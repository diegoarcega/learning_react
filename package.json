{
  "name": "learning_react",
  "version": "1.0.1",
  "description": "practising react",
  "main": "index.js",
  "scripts": {
    "start": "npm run dist",
    "test": "./node_modules/.bin/_mocha ./tests/index",
    "clean": "rimraf dist/ ",
    "copy": "copyfiles -f src/index.html src/favicon.ico dist",
    "prod": "npm run clean && webpack --config webpack.config.dist.js && npm run copy",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --exec ./node_modules/.bin/babel-node index.js",
    "dist": "NODE_ENV=production ./node_modules/.bin/babel-node index.js ",
    "kill": "fuser -k 30000/tcp && killall -9 node",
    "deploy": "surge --project ./dist --domain diegoarcega-react.surge.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diegoarcega/learning_react"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "engines": {
    "node": "6.3.0",
    "npm": "3.10.3"
  },
  "author": "Diego Arcega",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/diegoarcega/learning_react/issues"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "babel-cli": "^6.10.1",
    "babel-core": "~6.9.1",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-es2015-function-name": "^6.9.0",
    "babel-plugin-transform-es2015-literals": "^6.8.0",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "body-parser": "^1.15.2",
    "express": "~4.13.4",
    "express-winston": "^1.4.2",
    "highcharts": "^4.2.5",
    "highcharts-exporting": "^0.1.2",
    "highcharts-more": "^0.1.2",
    "highcharts-solid-gauge": "^0.1.2",
    "history": "^2.1.2",
    "material-ui": "^0.15.1",
    "mongodb": "^2.2.4",
    "muicss": "^0.6.5",
    "react": "~15.1.0",
    "react-dom": "~15.1.0",
    "react-highcharts": "^9.0.0",
    "react-redux": "~4.4.5",
    "react-router": "~2.5.2",
    "react-tap-event-plugin": "^1.0.0",
    "react-thunk": "^1.0.0",
    "redux": "~3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "file-loader": "^0.8.5",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "object-assign": "^4.1.0",
    "react-css-modules": "^3.7.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "~1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
